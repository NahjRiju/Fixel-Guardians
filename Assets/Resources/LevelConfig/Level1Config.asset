      voiceClip: {fileID: 8300000, guid: 95ad439b6ac02d84f8f773e4c2787ae8, type: 3}
  - characterName: Long
    characterImage: {fileID: 21300000, guid: 27e4f00f053e2b34081e7d0968497be1, type: 3}
    imagePosition: {x: 160, y: 130}
    imageRotation: {x: 0, y: 0, z: 0}
    imageScale: {x: 3, y: 2, z: 1}
    dialogueEntries:
